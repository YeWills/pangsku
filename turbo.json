{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/master",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "yalc": {
      "dependsOn": ["^build", "yalc"]
    },
    "build:extra": {
      "cache": false
    },
    "dev": {
      "cache": false
    },
    "test": {
      "outputs": [".jest-cache/**"]
    }
  },
  "globalDependencies": [
    "babel.config.json",
    "tsconfig.base.json",
    "tsconfig.json",
    "jest.config.ts"
  ]
}
