{
  "private": true,
  "scripts": {
    "yalc": "pnpm run --filter \"./packages/**\" yalc",
    "bootstrap": "psky-scripts bootstrap",
    "build": "psky-scripts turbo build",
    "build:extra": "psky-scripts turbo build:extra",
    "build:force": "psky-scripts turbo build --no-cache",
    "build:release": "psky-scripts turbo build --no-cache",
    "check:packageFiles": "psky-scripts checkPackageFiles",
    "dep:update": "pnpm up --interactive --latest --recursive",
    "dev": "psky-scripts turbo dev --parallel",
    "format": "prettier --cache --write .",
    "format:docs": "prettier --cache 'docs/**/*.{md,mdx}' --write --ignore-path .gitignore --ignore-unknown",
    "format:plugin-docs": "prettier --cache 'packages/plugin-docs/**/*' --write --ignore-unknown",
    "preinstall": "npx only-allow pnpm",
    "postinstall": "psky-scripts postinstall",
    "prepare": "husky install",
    "release": "psky-scripts release",
    "setup:webstorm": "psky-scripts setupWebStorm",
    "sync:tnpm": "psky-scripts syncTnpm",
    "tsc:check": "tsc --noEmit",
    "turbo:clean": "rimraf .turbo"
  },
  "lint-staged": {
    "*.{jsx,less,md,json}": [
      "prettier --cache --write"
    ],
    "*.ts?(x)": [
      "prettier --cache --parser=typescript --write"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.22.6",
    "@babel/core": "^7.22.6",
    "@babel/preset-env": "^7.22.6",
    "@babel/preset-typescript": "^7.22.5",
    "@pangskyjs/utils": "workspace:*",
    "@pnpm/lockfile-file": "^5.0.3",
    "@pnpm/logger": "^4.0.0",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^17.0.35",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.5",
    "@types/resolve": "^1.20.2",
    "@types/rimraf": "3.0.2",
    "@vercel/ncc": "0.33.3",
    "dts-packer": "^0.0.3",
    "expect-playwright": "^0.8.0",
    "git-repo-info": "^2.1.1",
    "husky": "^7.0.4",
    "lerna": "^4.0.0",
    "lint-staged": "^12.3.7",
    "matcher": "^5.0.0",
    "only-allow": "^1.1.0",
    "pangsky": "workspace:*",
    "prettier": "^2.7.1",
    "prettier-plugin-organize-imports": "^2.3.4",
    "prettier-plugin-packagejson": "^2.2.18",
    "psky-scripts": "workspace:*",
    "regenerator-runtime": "^0.13.9",
    "resolve": "^1.22.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.7.0",
    "tsx": "^3.8.0",
    "turbo": "^1.3.1",
    "typescript": "^4.8.4",
    "uglify-js": "^3.15.4",
    "zx": "^7.0.8"
  },
  "packageManager": "pnpm@7.3.0",
  "engines": {
    "node": ">=14",
    "pnpm": ">=7"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "dva",
        "postcss",
        "webpack",
        "eslint",
        "stylelint",
        "redux"
      ]
    }
  },
  "_local": "This flag is used to check if the development in local, Please do not delete."
}
